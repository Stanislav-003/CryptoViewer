<UserControl x:Class="Digital_Cloud_Technologies.Views.CryptoMemberInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Digital_Cloud_Technologies.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="this">
    <Border BorderBrush="Silver" BorderThickness="1">
        <DockPanel>
            <TextBlock FontSize="40" Text="{Binding ElementName=this, Path=Member.Name}" d:Text="1235"></TextBlock>
        </DockPanel>
    </Border>
</UserControl>
